#include<iostream>
#include<stdio.h>
#include<math.h>
#include<stdlib.h>
using namespace std;

int main() {
	int p[20];
	for (int i = 0; i < 20; i++) {
		p[i] = rand() % 100;
	}
	int n = 0;
	for (int i = 0; i < 20; i++) {
		cout << p[i] << " ";
	}
	cout << endl;

	while (n < 20) {
		int gotovo = 1;
		for (int i = 0; i < 19; i++) {
			if (p[i] % 2 == 0) {
				for (int ii = i + 1; ii < 20; ii++) {
					if (p[ii] % 2 == 0) {
						if (p[i] > p[ii]) {
							int pomocna;
							pomocna = p[i];
							p[i] = p[ii];
							p[ii] = pomocna;

						}
					}
				}
			}			
		}
		n++;
	}
	for (int i = 0; i < 20; i++) {
		cout << p[i] << " ";
	}

}
