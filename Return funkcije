#include <iostream>
#include <stdio.h>
#include <stdlib.h>
#include <math.h>
using namespace std;

float povrsina() {
    char izbor;
    cin >> izbor;
    double p;

    if (izbor == 'a') {
        double a;
        cin >> a;
        p = a * 3.14;
        return p;
    }

    if (izbor == 'b') {
        double a;
        double b;
        cin >> a>>b;
        p = a * b;
        return p;
    }

    if (izbor == 'c') {
        double a;
        cin >> a;
        p = a * a;
        return p;
    }
}

float zbroj() {
    int a, z = 0;
    cin >> a;
    while (a > 0)
    {
        z += a % 10;
        a /= 10;
    }
    return z;
}

float brojz() {
    int a, i = 0;
    cin >> a;
    while (a > 0)
    {
        a /= 10;
        i++;
    }
    return i;
}

float rastuce() {
    int a, n, d = 10, b;
    cin >> a;
    bool rast = true;
    n = a;
    while (n > 0)
    {
        a = n % d;
        n /= 10;
        b = n % d;
        if (a < b) {
            rast = false;
            break;
        }
    }
    return rast;
}


int main()
{
    cout << povrsina();
    cout << zbroj();
    cout << brojz();
    cout << rastuce();


}
