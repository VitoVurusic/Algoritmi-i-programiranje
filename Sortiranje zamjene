#include <iostream>
#include <stdio.h>
#include <stdlib.h>
#include <math.h>

using namespace std;
int main()
{
   
    int p[20] = { 112,47,38,128,587,12,29,173,190,28,154,487,395,237,1265,1587,187,198,200,892 };
    for (int k = 0; k < 10; k++) {
        int t = k;
        int n = k;
        for (t; t < 10; t++) {
            if (p[t] < p[n]) {
                n = t;
            }
            int x;
            x = p[k];
            p[k] = p[n];
            p[n] = x;
        }
        for (int i=0; i < 10; i++) {
            cout << p[i] << " ";

        }
        cout << endl;
    }
    for (int j = 10; k < 20; j++) {
        int z = j;
        int p = j;
        for (z; z < 20; z++) {
            if (p[z] > p[p]) {
                h = p;
            }
            int y;
            y = p[j];
            p[j] = p[p];
            p[p] = y;
        }
        for (int l = 10; l < 20; l++) {
            cout << p[l] << " ";

        }
        cout << endl;
    }

    return 0;
}
